  "components": {
    "schemas": {
      "Part": {
        "type": "object",
        "properties": {
          "Brand": {
            "type": "string",
            "example": "Toyota"
          },
          "Name": {
            "type": "string",
            "example": "Brake Pads"
          },
          "Quality": {
            "type": "string",
            "enum": ["OEM", "Aftermarket", "Used"],
            "example": "OEM"
          },
          "Vehicles": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["Corolla", "Camry"]
          },
          "CreatedAt": {
            "type": "string",
            "format": "date-time",
            "example": "2023-10-01T12:00:00Z"
          },
          "_id": {
            "type": "string",
            "example": "64f8b7e1b1b2c3d4e5f6a7b8"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {
          "Email": {
            "type": "string",
            "example": "user@example.com"
          },
          "FirstName": {
            "type": "string",
            "example": "John"
          },
          "LastName": {
            "type": "string",
            "example": "Doe"
          },
          "PasswordHash": {
            "type": "string",
            "example": "hashedpassword123"
          },
          "Username": {
            "type": "string",
            "example": "johndoe"
          },
          "AccountType": {
            "type": "string",
            "enum": ["admin", "customer"],
            "example": "customer"
          },
          "PhoneNumber": {
            "type": "string",
            "example": "123-456-7890"
          },
          "CreatedAt": {
            "type": "string",
            "format": "date-time",
            "example": "2023-10-01T12:00:00Z"
          },
          "_id": {
            "type": "string",
            "example": "64f8b7e1b1b2c3d4e5f6a7b8"
          }
        }
      },
      "Vehicle": {
        "type": "object",
        "properties": {
          "Brand": {
            "type": "string",
            "example": "Toyota"
          },
          "Description": {
            "type": "string",
            "example": "A reliable sedan."
          },
          "Engine_type": {
            "type": "string",
            "example": "Inline-4"
          },
          "Fuel_type": {
            "type": "string",
            "example": "Gasoline"
          },
          "Name": {
            "type": "string",
            "example": "Corolla"
          },
          "Transmission": {
            "type": "string",
            "example": "Automatic"
          },
          "Year": {
            "type": "integer",
            "example": 2023
          },
          "Type": {
            "type": "string",
            "example": "Sedan"
          },
          "colors_available": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["Red", "Blue", "White"]
          },
          "CreatedAt": {
            "type": "string",
            "format": "date-time",
            "example": "2023-10-01T12:00:00Z"
          },
          "_id": {
            "type": "string",
            "example": "64f8b7e1b1b2c3d4e5f6a7b8"
          }
        }
      }
    }
  }
